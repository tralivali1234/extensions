@using System.Configuration
@using Signum.Entities.Dashboard
@using Signum.Web.Dashboard
@using Signum.Entities.DynamicQuery

@using (var llp = Html.TypeContext<LinkListPartEntity>())
{
    <ul class="sf-cp-link-list">
        @foreach (LinkElementEmbedded link in Model.Value.Links)
        {
            <li>
                @Html.Href(Url.Content(link.Link), link.Label.DefaultText(link.Link))
            </li>
        }
    </ul>
}
